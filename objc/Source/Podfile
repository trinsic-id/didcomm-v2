source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '13.0'

install! 'cocoapods', :deterministic_uuids => false

target 'Okapi-iOS' do
  # Depend on the generated HelloWorld library.
  pod 'Okapi-Proto', :path => '.'
end

target 'Tests' do
  # Depend on the generated HelloWorld library.
  pod 'Okapi-Proto', :path => '.'
end